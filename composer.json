{
  "name": "msdevbytes/filament-currency-switcher",
  "description": "A Filament v4 plugin to add a currency switcher with automatic conversion",
  "type": "library",
  "version": "1.5.6",
  "license": "MIT",
  "keywords": [
    "Filament",
    "Currency Switcher",
    "Switcher",
    "Filament v4"
  ],
  "authors": [
    {
      "name": "Muhammad Shahbaz",
      "email": "shahbaz.khan.wip@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.0",
    "code-distortion/currency": "^0.5.6",
    "mallardduck/blade-boxicons": "^v2.2.0"
  },
  "autoload": {
    "psr-4": {
      "Msdevbytes\\CurrencySwitcher\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Msdevbytes\\CurrencySwitcher\\CurrencyServiceProvider"
      ]
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
